{
  "name": "hackateen",
  "version": "1.0.0",
  "description": "projeto hackateen",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "cls && node app.js",
    "install": "cls && npm install sqlite3 && npm install jsonwebtoken && npm install jest",
    "test-global": "cls && jest --detectOpenHandles",
    "start-app": "node -e \"import('./app.js').then(({ App }) => App())\"",
    "test-users": "cls && jest -i tests/users.test.js -t",
    "test-comments": "cls && jest -i tests/comments.test.js -t",
    "test-posts": "cls && jest -i tests/posts.test.js -t"
  },  
  "keywords": [
    "Hackateen"
  ],
  "author": "Arthur Martin, Ryan Ferreira, Moises Lima",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  }
}
